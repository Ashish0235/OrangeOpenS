<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <!-- Adding listeners -->
    <listeners>
        <listener class-name="utils.SuitListner" />
    </listeners>
    
    <!-- Test with Chrome -->
    <test name="TestwithChrome">
        <!-- Parameter for browser -->
        <parameter name="browser" value="chrome" />
        
        <!-- Test classes -->
        <classes>
            <class name="testClassess.Logout_Test" />
            <class name="testClassess.Dashboard_Test" />
        </classes>
    </test>
</suite>
